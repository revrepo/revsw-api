{
  "log_config": {
    "transports": {
      "Console": {
        "silent": true
      },
      "File": {
        "filename": "./log/revsw-api.log",
        "level": "debug"
      },
      "Rsyslog": {
        "Level": "info",
        "app-name": "revsw-api"
      }
    }
  },
  "service": {
    "http_port": 80,
    "https_port": 8000
  },
  "key_path": "./config/ssl_certs/server.key",
  "cert_path": "./config/ssl_certs/server.crt"
}
